{
  "name": "work",
  "version": "1.0.0",
  "description": "",
  "main": "",
  "scripts": {
    "start": "yarn bs & yarn watch:ts & yarn watch:sass & yarn develop:pug & yarn watch:html",
    "build": "yarn product:ts && yarn product:sass && yarn product:pug",
    "watch:ts": "onchange './src/ts' -- yarn develop:ts",
    "product:ts": "webpack --config ./config/webpack.config.js --mode production",
    "develop:ts": "webpack --config ./config/webpack.config.js --mode development",
    "lint:ts": "yarn tslint ./src/ts/**/*.ts",
    "product:pug": "pug ./src/pug --hierarchy -o ./htdocs",
    "develop:pug": "pug ./src/pug --hierarchy -o ./htdocs -P -w",
    "watch:sass": "onchange './src/sass' -- yarn develop:sass",
    "develop:sass": "gulp kySass --env development",
    "product:sass": "gulp kySass --env production",
    "watch:html": "onchange './htdocs/**/{*.htm,*.html,*.php}' -- yarn htmlhint",
    "htmlhint": "htmlhint ./htdocs/**/{*.htm,*.html,*.php}",
    "imagemin": "gulp kyImagemin",
    "audioSprite": "gulp kyAudioSprite",
    "bs": "browser-sync start --config ./config/bs-config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
